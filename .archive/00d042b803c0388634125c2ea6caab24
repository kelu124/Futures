    function getRefreshableUnits() {
        var refreshableUnits = [];
        if (gptAdSlots
            && gptAdSlots[0]
            && gptAdSlots[1]
        ) {
            var headerBanner = document.getElementById(gptAdSlots[0].getSlotElementId());
