                refreshableUnits.push(gptAdSlots[0]);
            }
            if (sideBannerMPU && 'true' === sideBannerMPU.dataset.viewable) {
                refreshableUnits.push(gptAdSlots[1]);
            }
        }
        return refreshableUnits;
    }

