    var siteMapStorageKey = '/content/main/en';

    var currentPath = '/en/news-and-events/eth-news/news/2020/02/cybathlon-2020-start';

    window.ethSitemap = {};

    window.ethSitemap.loaded = new Promise(function (resolve, reject) {
      var xmlhttp = new XMLHttpRequest();

      xmlhttp.onreadystatechange = function() {
        if (xmlhttp.readyState == XMLHttpRequest.DONE) {
          if (xmlhttp.status == 200) {
            if (xmlhttp.responseText) {
              window.ethSitemap.data = JSON.parse(xmlhttp.responseText);
              resolve(window.ethSitemap.data);
              window.sessionStorage.setItem(siteMapStorageKey, xmlhttp.responseText)
            }
          } else {
            reject();
          }
        }
      };

      xmlhttp.open("GET", "/en/.navigation.json", true);
      xmlhttp.send();
    });

