  if(url && url.length && url.length===4 && url[1]==='manuals'){
  	document.querySelector('.logo-icon').src='https://assets.rebelmouse.io/eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJpbWFnZSI6Imh0dHBzOi8vYXNzZXRzLnJibC5tcy8yNTYyNTQxMS9vcmlnaW4ucG5nIiwiZXhwaXJlc19hdCI6MTY0NDYzMDU2OH0.UKk40MUtDMxMAkL7Z6hy6vNFS6mHQqfMQ2X0dZbTqQw/img.png?width=980';
