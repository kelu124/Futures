     text-align: center;
     width: 100%;
     color: #B94A48;
     font-family: sans-serif;
     font-size: 12px;
     line-height: 14.5px;
     padding: 0;
     position: absolute; 
     left: 0; 
     right: 0;
     top: 105%;
  }
  #MC_resp a {
    display: none;
  }
