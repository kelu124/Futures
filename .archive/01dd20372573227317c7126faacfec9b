      <!-- ZEPHR_FEATURE article --><!-- End User Code --><div id="sPost_0_0_5_0_49_5_0_1" class="post-partial article__splash-custom"><div class="post-splash-custom clearfix"  data-attr-id="sPost_0_0_5_0_49_5_0_1" data-attr-layout_quality="1" data-attr-format="splash-custom" data-attr-all_element_order="headline,section,badges_sponsored,body,post_shares,author,date,photo_caption,photo_credit,subheadline,community_name,badges,snark_line,page_views,follow_button,community_comments,like_button,source_link,collection_button,tags,primary_tag,main_author,all_sections,site_field_twitter,site_field_facebook,site_field_email,site_field_linkedin,custom_field_presentedbyimg,custom_field_podcasturl,custom_field_presentedbylink,date_modified,product_prices,product_vendor,product_buy_link,words_count,time_to_read,custom_field_registrationWall" data-attr-layout_body="bottom" data-attr-layout_post_shares="bottom" data-attr-all_share_buttons="Facebook,Twitter,Linkedin,Email,Separator,Tumblr,GooglePlus,Pinterest,Whatsapp,Reddit,CopyLink,Flipboard,SMS,Slack" data-attr-layout_Linkedin="active" data-attr-layout_Separator="active" data-attr-layout_Email="active" data-attr-layout_all_image_crop="16x9" data-attr-layout_all_video_crop="original" data-attr-layout_all_date_format="%B %-d, %Y" data-attr-layout_badges_sponsored="top" data-attr-show_full_post_body="true" data-attr-layout_Tumblr="inactive" data-attr-layout_GooglePlus="inactive" data-attr-layout_Pinterest="inactive" data-attr-layout_Whatsapp="inactive" data-attr-layout_Reddit="inactive" data-attr-layout_CopyLink="inactive" data-attr-layout_Flipboard="inactive" data-attr-layout_SMS="inactive" data-attr-layout_Slack="inactive" data-attr-layout_section="top" data-attr-layout_headline="top" data-attr-header_template="jinja/post/custom-css.html">
