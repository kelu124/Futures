window.REBELMOUSE_ACTIVE_TASKS_QUEUE.push(function(){

    
        function findFirstChildByClass(element, className) {
        var foundElement = null, found;
        function recurse(element, className, found) {
            for (var i = 0; i < element.childNodes.length && !found; i++) {
                var el = element.childNodes[i];
                var classes = el.className != undefined? el.className.split(" ") : [];
                for (var j = 0, jl = classes.length; j < jl; j++) {
                    if (classes[j] == className) {
                        found = true;
                        foundElement = element.childNodes[i];
                        break;
                    }
                }
                if(found)
                    break;
                recurse(element.childNodes[i], className, found);
            }
        }
        recurse(element, className, false);
        return foundElement;
    }

var breakingNews=document.querySelector(".breaking-news");
var breakingNewsElement=findFirstChildByClass(breakingNews,'widget__headline-text');
if(!window.localStorage.getItem("breakingNewsShown") && breakingNewsElement){
  if(breakingNewsElement && breakingNewsElement.innerText){
  	window.localStorage.setItem("breakingNewsShown",true);
	window.localStorage.setItem("breakingNews",breakingNewsElement.innerText);
  }
}
console.log(window.localStorage.getItem("breakingNewsShown"));
console.log(window.localStorage.getItem("breakingNews"));
if(window.localStorage.getItem("breakingNewsShown")==="true"){
	breakingNews.classList.add('breaking-news-open');
}
var child=findFirstChildByClass(breakingNews,'widget__body');
var node = document.createElement("SPAN");
var textnode = document.createTextNode("X");
if(node && node.appendChild){
	node.appendChild(textnode);
}
node.classList.add('breaking-news-close');
node.addEventListener('click',function(){
	breakingNews.classList.remove('breaking-news-open');
    window.localStorage.setItem("breakingNewsShown",false);
});
if(child && child.appendChild){
	child.appendChild(node);
}

window.addEventListener('locationchange', function(){
    var getText=findFirstChildByClass(breakingNews,'widget__headline-text');
  	if(getText && getText.innerText && getText.innerText!==window.localStorage.getItem("breakingNews")){
    	document.querySelector(".breaking-news").classList.add('breaking-news-open');
      window.localStorage.setItem("breakingNews",getText.innerText);
      	window.localStorage.setItem("breakingNewsShown",true);
    }
});
    

});

