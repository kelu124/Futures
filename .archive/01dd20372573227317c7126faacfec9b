document.body.classList.toggle("custom-search-opened");
}
</script><!-- End User Code --><!-- User Code --><div data-section="newsletter-topbar"><!-- End User Code --><div class="subscribe-button-wrapper" id="sTop_Bar_0_0_9_0_0_20_0_3_1_1_1"><div class="text-element" id="sTop_Bar_0_0_9_0_0_20_0_3_1_1_1_0"><a class="color-inherit" href="/newsletters/" target="_self">Subscribe</a></div></div><!-- User Code --></div><!-- End User Code --></div><div class="site-links" id="sTop_Bar_0_0_9_0_0_20_0_4"><div class="list" id="sTop_Bar_0_0_9_0_0_20_0_4_0"><a class="list__item" href="/workplace/" target="_self">Workplace</a><a class="list__item" href="/enterprise/" target="_self">Enterprise</a><a class="list__item" href="/fintech/" target="_self">Fintech</a><a class="list__item" href="/china/" target="_self">China</a><a class="list__item" href="/policy/" target="_self">Policy</a><a class="list__item" href="/newsletters/" target="_self">Newsletters</a><a class="list__item" href="/braintrust/" target="_self">Braintrust</a><a class="list__item" href="/podcasts/source-code/" target="_self">Podcast</a><a class="list__item" href="/events/" target="_self">Events</a><a class="list__item" href="/careers" target="_self">Careers</a><a class="list__item" href="/st/about_us" target="_self">About Us</a></div></div><span class="js-brick-description hidden" data-name="__social_share_element"></span><!-- User Code --><div class="site-share-placeholder" aria-role="list" aria-label="share-site"><a href="https://twitter.com/protocol" class="twitter"  rel="noopener noreferrer"><svg class="icon"><use xlink:href="#tw-icon"></use></svg></a><a href="https://www.linkedin.com/company/protocol-media/" class="linkedin"  rel="noopener noreferrer"><svg class="icon"><use xlink:href="#in-icon"></use></svg></a><a href="https://www.facebook.com/protocol/" class="facebook"  rel="noopener noreferrer"><svg class="icon"><use xlink:href="#facecbook-svg-circle"></use></svg></a><a href="https://www.instagram.com/protocol/" class="instagram"  rel="noopener noreferrer"><svg cla