window.REBELMOUSE_ACTIVE_TASKS_QUEUE.push(function(){

    
        var stickySahreContainer = document.querySelector(".post-partial .widget__body ~ .widget__body");
if(document.querySelector(".default-layout-post")) return;
if(stickySahreContainer){
//var offsetElement=stickySahreContainer.querySelector(".body");
var offsetElement=document.querySelector(".body");
var stickyShareElement=document.querySelector(".post-partial .widget__shares");
var stickySharerHeight= stickyShareElement.clientHeight;
var leftoffsetValue = stickySahreContainer;
var topValueToCheck= 138;//topbar and share position from top
var bottomValuetoCheck= 296;
var innerWidth=window.innerWidth;
var leftShareOffsetValue=innerWidth > 900 ? (( offsetElement.getBoundingClientRect().left-80 )): 20;
var setSharePosition = Ithrottle(function() {
//console.log("top:"+ stickySahreContainer.getBoundingClientRect().top+"---- bottom:"+ stickySahreContainer.getBoundingClientRect().bottom )
    if(offsetElement.getBoundingClientRect().top < topValueToCheck && stickySahreContainer.getBoundingClientRect().bottom > bottomValuetoCheck) {
        stickyShareElement.style.position="fixed";
        stickyShareElement.style.top= topValueToCheck+"px";
        stickyShareElement.style.left= leftShareOffsetValue +"px";

    }
   else if(offsetElement.getBoundingClientRect().top < topValueToCheck && stickySahreContainer.getBoundingClientRect().bottom < bottomValuetoCheck) {
        stickyShareElement.style.position="absolute";
        stickyShareElement.style.top= "auto";
        stickyShareElement.style.bottom= "0";
        stickyShareElement.style.left= "";

    }
    else {
        stickyShareElement.removeAttribute("style")
    }
    
}, 100);

if(window.innerWidth > 768){
window.addEventListener("scroll", setSharePosition);
window.addEventListener("resize" ,function(){
    leftShareOffsetValue =window.innerWidth > 900 ? (( offsetElement.getBoundingClientRect().left-80 )): 20;
})
}
}
    

});</script>
    <script id="user-javascript-lowest" type="text">

window.REBELMOUSE_LOWEST_TASKS_QUEUE.push(function(){

    
        /**********************************************************
 * this code should only affect the naavbar. Goal is to make it be a big drop down for desktop
 * when you hover and on mobile a menu that slides over when you click on the hamburger
 * by eric blom
 **********************************************************/

 const addShowClass = (el)=>{
    if(el.children && el.children.length && el.children[1]){
        el.children[1].classList.add('unhide')
    }
}
const removeShowClass = (el)=>{
    if(el.children.length && el.children[1]){
        el.children[1].classList.remove('unhide')
    }
}

const showList = (els)=>{
    els.forEach((el)=>{
        addShowClass(el)
    })
}

const closeList = (els)=>{
    // hide the list and rotate the button
    els.forEach((elI)=>removeShowClass(elI))
}

const toggleDisplay = (x)=>{
  if (x.style.display === "none") {
    x.style.display = "inherit";
  } else {
    x.style.display = "none";
  }
} 
const hide = (className, className2, className3)=>{
    var element = document.getElementsByClassName(className)[0];
    element.classList.toggle('moved-offscreen')
    element.classList.toggle('wrapper-expanded')
    var element2 = document.getElementsByClassName(className2)[0];

    element2.classList.toggle('hidden-subsections')
    element2.classList.toggle('blue-expanded')
    const icons = document.getElementById("topbar-menu-toggle");
    for(let i=0;i<icons.children.length;i++){

      // toggleDisplay(icons.children[i])
      icons.children[i].classList.toggle('close')
    }
}
const showListMobile = (el)=>{
    el.classList.toggle('unhide-mobile')
}
const createExpandButton = (el)=>{
    // on mobile, click this to expand
    // on larger hover to expand
    let div = document.createElement('button');
    div.textContent = ">";
    div.classList.add('expand-button');
    div.onclick = function(){ showListMobile(el); div.classList.toggle('expanded-menu-mobile')}

    return div;
}
const moveNode = (toMoveClass, targetClass) =>{
    let toMove = document.getElementsByClassName(toMoveClass)[0]
    let target = document.getElementsByClassName(targetClass)[0]
    let line = document.createElement("div");
    line.classList.add('hr-line')
  	// if(!toMove.classList.contains('events-menu')){
    //   target.appendChild(line);
    // }
    target.appendChild(line);
	target.appendChild(toMove);
  	toMove.classList.remove('put-in-dropdown') // take away class taht makes display none
    toMove.classList.add('menu-subsections') // make it only appear on hover
    toMove.classList.add('moved-menu')
}

var topLevelMenu = document.getElementsByClassName('new-top-navbar')[0]; 
var menuButton = document.getElementById('topbar-menu-toggle');
console.log('testing', 'menuButton', menuButton);
menuButton.onclick = function() {
    hide('new-top-navbar', 'top-bar-links');
};

const topBar = document.getElementsByClassName('top-bar-links')[0]
const topBarLinks = Array.from(topBar.children)
topBarLinks.forEach( element => {
    if(element.children.length ){
        element.children[0].classList.add('section-link')

        if(element.children[1]){ // has subsections
            
            element.children[1].classList.add('subsection-link') // list of subsections
            element.children[1].classList.add('hide') // hide them to start

            const expandButton = createExpandButton(element.children[1]);
            if(element.children[0].nodeName != "A"){ // don't add to log in link
               element.children[0].appendChild(expandButton)
             }
            
        }
        
    }
})

if (window.innerWidth > 1023){ // move elements on non-mobile
  // move nodes
  var firstParent = 'china-menu';
  moveNode('braintrust-menu', firstParent)
  moveNode('power-index-menu', firstParent)
  moveNode('manuals-menu', firstParent)
  moveNode('bulletins-menu', firstParent) 

  var secondParent = 'climate-menu';
  moveNode('events-menu', secondParent) 
  moveNode('calendars-menu', secondParent)
  moveNode('podcast-menu', secondParent)
  moveNode('company-menu', secondParent) 
  moveNode('joinus-menu', secondParent)
  moveNode('login-wrapper', secondParent)
}
    

});</script>
