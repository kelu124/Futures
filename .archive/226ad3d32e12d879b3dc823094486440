  window.errorTracker(
    function(error) {
      window._400.push({key:'page.error.javascript', value:error});
    },
    {
      scriptFilter: new RegExp('^/|^' + location.protocol + '//' + location.host),
      version: "ab58073dce7f8df9a5684f4d2b9bdfa1b414ee08",
      versionTimestamp: "1665006205706"
    });
