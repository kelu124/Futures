        <span class="social-date__text">October 2, 2022</span>
    </div></div></div></div>
    
    


<script type="application/json" id="post-context-2658362553" class="post-context ">
    {"customDimensions": {"1":"Hirsh Chitkara","7":2658362553,"3":"book, q\u0026a, semiconductors, china, u.s., war, weapons, military, chips act, chips","6":"chips","2":"policy","4":"10/02/2022"}, "post": {"split_testing": {}, "providerId": 0, "sections": [0, 473310849, 545659540], "buckets": [], "authors": [20878683]} }
