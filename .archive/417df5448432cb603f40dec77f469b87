						:aria-expanded="flyout"
						x-init = "
							$watch('flyout', value => {
								if (value == true) {
									document.body.classList.add('overflow-hidden');
									document.getElementsByTagName('html')[0].classList.add('overflow-hidden');
								}
								else {
									document.body.classList.remove('overflow-hidden');
