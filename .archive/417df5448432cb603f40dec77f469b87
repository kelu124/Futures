<script>

function shareThis(elem){

  var shareUrl = "http://entm.ag/1uUx0uk",
    shareCanonicalUrl = "https://www.entrepreneur.com/article/240254",
    shareAuthorHandle = "ivanmisner",
    shareDeck = "Your distractions are keeping you from being alert, attentive and focused and can hamper your post-event follow up not to mention your day t...",
    shareTitle = "The Danger of Continuous Partial Attention",
    twitterHandle = "ivanmisner"
    twitterBool = "us",
    edition = "us",
    socialUrl;

  if (!elem.getAttribute("title")) {
    return;
  }

  var network = elem.getAttribute("title").toLowerCase();
  var viaText = (edition == 'es-us') ? ' vía @' : ' via @'; // U+00ED code for í

  console.log('network');
  console.log(network);
  switch (network) {
    case "twitter":
      socialUrl = "https://twitter.com/intent/tweet?text=" + encodeURI(shareTitle).replace(/#/g, '%23').replace(/&/g, '%26') + viaText + twitterHandle + ' ' + shareUrl;
    break;

    case "facebook":
      socialUrl = "https://www.facebook.com/sharer/sharer.php?u=" + shareUrl
    break;

    case "linkedin":
      var socialUrl = "https://www.linkedin.com/sharing/share-offsite/?url=" + shareUrl;
    break;

    case "copy":
      var toast = document.getElementById('shareNotification');
      var toastMessage = document.getElementById('shareNotification-message');
      var toastIcon = document.getElementById('shareNotification-icon');

      navigator.clipboard.writeText(shareUrl).then(function() {
        toast.style.display = "block";

        setTimeout(() => {
          toast.style.display = "none";
        }, 3000);

      }, function(err) {
        toast.style.display = 'block';
        toastMessage.innerHTML = 'An error has occurred in trying to copy the link';
        toastIcon.innerHTML = '<svg xmlns="http://www.w3.org/2000/svg" class="h-6 w-6 text-red-500 mr-1" viewBox="0 0 24 24" fill="currentColor"><path d="M0 0h24v24H0z" fill="none"/><path d="M12 2C6.48 2 2 6.48 2 12s4.48 10 10 10 10-4.48 10-10S17.52 2 12 2zm1 15h-2v-2h2v2zm0-4h-2V7h2v6z"/></svg>';

        setTimeout(() => {
          toast.style.display = "none";
        }, 3000);
      });

    break;

    default:
      return false;
  }

  if(!socialUrl) {
    return false;
  }

  window.dataLayer = window.dataLayer || [];
  window.dataLayer.push({
    'event' : 'social-share',
    'social_platform': network
  });

  newwindow = window.open(socialUrl,'name','height=600, width=650, top=0, right=0, toolbar=0, location=0, menubar=0, directories=0, scrollbars=0');
  if (window.focus) {
    {newwindow.focus()}
  }

  return false;
};


(function() {
  const userAgent = navigator.userAgent || navigator.vendor || window.opera;
  const iosSVG = '<svg aria-label="Share this icon" xmlns="http://www.w3.org/2000/svg" class="h-5 w-5 share-button-svg" viewBox="0 0 24 24" fill="currentColor"><path d="M0 0h24v24H0V0z" fill="none"/><path d="M16 5l-1.42 1.42-1.59-1.59V16h-1.98V4.83L9.42 6.42 8 5l4-4 4 4zm4 5v11c0 1.1-.9 2-2 2H6c-1.11 0-2-.9-2-2V10c0-1.11.89-2 2-2h3v2H6v11h12V10h-3V8h3c1.1 0 2 .89 2 2z"/></svg>';
  const shareButtonCont = document.querySelector('.share-button-svg');
  const shareButton = document.querySelector('.share-button');
  const container = document.getElementById('nativeshare');
  // iOS detection, change icon
  if (/iPad|iPhone|iPod/.test(userAgent) && !window.MSStream) {
    shareButtonCont.innerHTML = iosSVG;
  }

  shareButton.addEventListener('click', event => {
    event.preventDefault();
    if (navigator.share) {
      navigator.share({
        title: 'The Danger of Continuous Partial Attention',
        url: 'http://entm.ag/1uUx0uk'
      }).then(() => {
        console.log('Thanks for sharing!');
      })
      .catch(console.error);
    } else {
      // Fallback
      document.getElementById('nativeshare').style.display = 'block';
      console.log('***** no native share support');
    }
  });

  if (!navigator.share) {
    document.addEventListener('click', function(e) {
      if (!container.contains(e.target) && !shareButton.contains(e.target)) {
        container.style.display = 'none';
      }
    });
  }
})();

</script>

