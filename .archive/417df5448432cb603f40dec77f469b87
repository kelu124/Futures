						<button @click="flyout = !flyout; moreMenuOpen = false;" :class="{ 'text-gray-900': flyout, 'text-gray-500': !flyout }" class="ml-1 p-3 h-14 inline-flex items-center justify-center text-gray-700 hover:text-gray-600 hover:bg-gray-100 focus:outline focus:bg-gray-100 focus:text-gray-500 transition duration-150 ease-in-out" onclick="mobileMenu()" id="mobile-menu" aria-expanded="false" aria-label="open">
							<!-- Menu open: "hidden", Menu closed: "block" -->
