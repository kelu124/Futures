			<script>
			// logic for viewable ads
			window.onload = function () {
				if (window.googletag && googletag.apiReady) {

					googletag.pubads().addEventListener('slotRenderEnded', function(e) {
						var slot =  e.slot.getSlotElementId(),
							slotWidth  = (!Array.isArray(e.size)) ? null : e.size[0],
							slotHeight = (!Array.isArray(e.size)) ? null : e.size[1],
							adslot = document.getElementById(e.slot.getSlotElementId());

						var adType = adslot.getAttribute("data-key");

						if(adType == 'a2ad' && slotWidth == 970) {
							adslot.style.width = '100vw';
							adslot.style.marginLeft = 'calc(50% - 50vw)';
						}
					});

					googletag.pubads().addEventListener('impressionViewable', function(e) {
						var slot = e.slot;
						// console.log('Impression for slot', slot.getSlotElementId(), 'became viewable.');

						var adslot = document.getElementById(slot.getSlotElementId());

						if(adslot.classList.contains('stickyTopAd')) {
							adslot.classList.remove('sticky');
						}
					});
				}
			}
