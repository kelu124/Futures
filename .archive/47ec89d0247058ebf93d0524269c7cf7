						onFill: function(data) {
							getFormattedDate(data);
						},
						onError: function(error) { }
					}]);
				
			break;

			case 'Article':
			case 'PrintArticle':
			case 'PartnerArticle':
			case 'BlogPost':
				
					q().push(["insertPreview", {
						label: "ArticleLatest",
						unit: {
							"server": "dfp",
							"id": DFP_id,
							"size": "2x2",
							"targets": renderTagerting('n-article')
						},
						location: "#read-next > div.aside-block__body > div.related-article-group > div:nth-child(1)",
						infoText: "",
						infoButtonText: "",
						template: compiledTemplateArticleLatest,
						onRender: function($element) {
							var parent = document.getElementById('read-next');
							var parentBody = parent.children[1].children[0];
							parentBody.removeChild(parentBody.lastChild);
						},
						onFill: function(data) {
							getFormattedDate(data);
						},
						onError: function(error) { }
					}]);
				
			break;
		}

