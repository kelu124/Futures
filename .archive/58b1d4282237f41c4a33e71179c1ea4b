  _iub.csConfiguration.callback = 
{
  onReady: function () {
    console.log("[NA] Iubenda onReady", _iub.cc , window.performance.now());

    if (_iub.cs.api.ccpaApplies()) {
      function addCcpaLink() {
        console.log("[NA] addCcpaLink", window.performance.now());

        var ccpaLink = document.createElement("a");
        ccpaLink.href = "#open-ccpa-settings";
        ccpaLink.textContent = "Do Not Sell My Information";
        ccpaLink.classList.add("NavigationLink");
        ccpaLink.onclick = function (event) {
          console.log("Open CCPA settings");
          window._iub.cs.api.askCcpaOptOut();
          event.preventDefault();
        };

        var listItem = document.createElement("li");
        listItem.classList.add("FooterNavigationItem-items-item");
        listItem.append(ccpaLink);

        var privacyLink = document.querySelector('.FooterNavigationItem-items a[href*="/privacy/"]');
        try {
          privacyLink.parentElement.insertAdjacentElement("afterend", listItem);
        } catch (error) {
          console.error(error);
        }
      }

      addCcpaLink();
    }
  },
  onStartupFailed: function (err) {
    console.error(
      "[NA] Iubenda onStartupFailed",
      err,
      window.performance.now()
    );
    if (window.Sentry) {
      Sentry.captureException(err, {
        tags: {
          cc: "FR",
          region: "IDF",
        }
      });
    }
    else console.error("[NA] no Sentry", window.performance.now());
  },
}

</script>
