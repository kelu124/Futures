        link.setAttribute('href', 'https://fonts.googleapis.com/css?family=Open+Sans:400,500,600,700|Open+Sans:400,500,600,700|Lora:400,500,600,700');
        var relList = link.relList;

        if (relList && relList.supports('preload')) {
