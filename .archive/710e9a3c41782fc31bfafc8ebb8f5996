            tp.push(["setUsePianoIdUserProvider", true ]);
                                
        
                var tags = [];
                                    tags.push("cities and climate change");
                                    tags.push("climate adaptation");
                                    tags.push("climate crisis");
                                    tags.push("extreme heat");
                                    tags.push("extreme weather");
