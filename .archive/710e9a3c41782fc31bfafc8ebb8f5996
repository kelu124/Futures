  <script>
  function getPianoCookie() {
    var name = "abrPianoUid" + "=";
    var ca = document.cookie.split(';');
    for(var i = 0; i < ca.length; i++) {
      var c = ca[i];
      while (c.charAt(0) == ' ') {
        c = c.substring(1);
      }
      if (c.indexOf(name) == 0) {
        return c.substring(name.length, c.length);
      }
    }
    return "";
  }
  var abrPianoUid = getPianoCookie();
  !function(f,b,e,v,n,t,s)
