<script>
  window.delayOptimizelyForVid = true;
  var supportsPreload = window.supportsPreload;
  if (!supportsPreload) {
    // Load styles and scripts for  video
    var videoScript = window.document.createElement('script');
    videoScript.src = 'https://video-api.wsj.com/api-video/player/v3/js/video.min.js';
    document.head.appendChild(videoScript);

    var videoCSS = window.document.createElement('link');
    videoCSS.rel = 'stylesheet';
    videoCSS.type = 'text/css';
    videoCSS.href = 'https://video-api.wsj.com/api-video/player/v3/css/video.min.css';
    videoCSS.media = 'all';
    document.head.appendChild(videoCSS);

    var videoInit = window.document.createElement('script');
    videoInit.src = 'https://asset.barrons.com/article/public/video.08ce09b69d3c.js';
    document.head.appendChild(videoInit);
  }
</script>

