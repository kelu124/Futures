    var CHART_ENGINE_NAME = 'CustomDataChart_WSJ_0_9_5';
    var definitions = {};

    function loadScript(src) {
      var script = document.createElement('script');
      script.setAttribute('src', src);
      document.body.appendChild(script);
    }

    function loadCSS(cssURL){
      if(cssURL){
        const link = document.createElement('link');
        link.rel  = 'stylesheet';
        link.type = 'text/css'
        link.href = cssURL;
        document.head.appendChild(link);
      }
      return;
    }

    function checkIfScriptsAreComplete() {
      for (var key in definitions) {
        if (definitions.hasOwnProperty(key)) {
          if (!window[key]) {
            return setCheckCompleteTimeout();
          }
        }
      }
      init();
    }

    function setCheckCompleteTimeout() {
      return setTimeout(checkIfScriptsAreComplete, 100);
    }

