        (function() {
          $('.js-membership-footer-banner a').click(function(e) {
              gtag('event', 'select_content', {
                "promotions": [
                  {
                    'id': 'onsite_member_promo',
                    'name': 'Membership',
                    'creative': 'banner',
                    'position': 'footer_see_all'
                  }
                ]
              });

              gtag('event', 'click', {
                'event_category': 'Internal Promotions',
                'event_label': 'Membership'
              });
            });
        })();
      </script>
