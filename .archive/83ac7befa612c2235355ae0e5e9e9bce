        $('bookCover_26114433').observe('prototip:shown', function () {
          $$('div.prototip').each(function (e) {
            if ($('bookCover_26114433').hasClassName('ignored')) {
              e.setStyle({'display': 'none'});
              return;
            }
            e.setStyle({'overflow': 'visible'});
          });
