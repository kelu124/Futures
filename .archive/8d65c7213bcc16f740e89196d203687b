//old guys
		//document.getElementById("high1").src = "https://web.archive.org/web/20100821041938/http://cdn.okcimg.com/blog/your_looks_and_inbox/thefishcase.jpg";
		document.getElementById("high2").src = "https://web.archive.org/web/20100821041938/http://cdn.okcimg.com/blog/your_looks_and_inbox/angelboyxxx.jpg";
		document.getElementById("highLabel").innerHTML = "show women instead";
	}
	else {

		document.getElementById("high1").src = "https://web.archive.org/web/20100821041938/http://cdn.okcimg.com/blog/your_looks_and_inbox/ghostttt.jpg";
		document.getElementById("high2").src = "https://web.archive.org/web/20100821041938/http://cdn.okcimg.com/blog/your_looks_and_inbox/roomtodance.jpg";
		document.getElementById("highLabel").innerHTML = "show men instead";

	}


}


