function switchHigh(){

	var label = document.getElementById("highLabel").innerHTML;

	if (label == "show men instead") {

//new guys
		document.getElementById("high1").src = "https://web.archive.org/web/20100821041938/http://cdn.okcimg.com/blog/your_looks_and_inbox/charleybears.jpg";
