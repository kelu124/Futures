function pageScroll() {
    var x = document.viewport.getDimensions().height + document.viewport.getScrollOffsets().top - (entry.getHeight() + entry.cumulativeOffset().top);
    if (x > 0) {
        if (parseInt(share_popout.getStyle('top')) != 0)
            share_popout.morph({top: '0px'}, {duration: 0.25});
    }
    else {
        if (parseInt(share_popout.getStyle('top')) != -115)
            share_popout.morph({top: '-115px'}, {duration: 0.25});
    }
}

// The variable 'float_top' is set in footer.html, after the div that it depends on is put into the page
// (not used anymore; get rid of when the new social stuff that pops down from the top launches)
function pageScroll_old() {
	var float = document.getElementById('floating');
	var scroll_top = document.body.scrollTop;
	if (scroll_top == 0) {
		if (window.pageYOffset)
			scroll_top = window.pageYOffset;
		else
			scroll_top = (document.body.parentElement) ? document.body.parentElement.scrollTop : 0;
	}
	if (typeof respond_top != 'undefined' && scroll_top + float.offsetHeight + 40 > respond_top) {
		float.style.position = 'static';
		float.style.top = 'none';
		float.style.marginTop = (respond_top - float_top - float.offsetHeight) + 'px';
//		float.style.marginTop = (scroll_top - float_top) + 'px';
		return;
	}
	else if (scroll_top > float_top - 10) {
		float.style.position = 'fixed';
		float.style.top = '10px';
		float.style.marginTop = '0';
		//float.style.marginTop = (scroll_top - float_top + 10) + 'px';
	}	
	else {
		float.style.position = 'static';
		float.style.top = 'none';
		float.style.marginTop = '0';
	}
}

// Used for the green box in the sidebar
function selectSection (s) {
	var sections = ['favorites', 'all'];
	for (var i = 0; i < sections.length; i++) {
		if (sections[i] == s) {
			document.getElementById('section_' + sections[i]).style.display = 'block';
			document.getElementById('section_li_' + sections[i]).className = 'active';
		}
		else {
			document.getElementById('section_' + sections[i]).style.display = 'none';
			document.getElementById('section_li_' + sections[i]).className = '';
		}
	}
}



</script>

