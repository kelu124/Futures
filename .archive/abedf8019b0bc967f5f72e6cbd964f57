<script>function documentIsReady(functionToRunWhenReady) {
    if (document.readyState === 'complete') {
        functionToRunWhenReady()
    } else {
        document.addEventListener('DOMContentLoaded', functionToRunWhenReady)
    }
}

var trackingMethods = ['trackLockedDocumentViewed', 'trackCtaClicked', 'trackRelatedContentClicked', 'trackContentViewedWithinAPage']

createTrackingQueue('amplitudeService', trackingMethods)

var _crossPromotions = []

window.amplitudeService.trackLockedDocumentViewed({ crossPromotions: _crossPromotions })

documentIsReady(function () {
    var $breadcrumbSchool = document.querySelector('#breadcrumbs-school')
    $breadcrumbSchool && $breadcrumbSchool.addEventListener('click', function () {
        window.amplitudeService.trackCtaClicked({
            actionType: 'Link',
            text: 'Breadcrumb: ' + $breadcrumbSchool.innerText,
            intent: 'redirect',
            destinationURL: $breadcrumbSchool.href,
        })
    })

    var $breadcrumbDepartment = document.querySelector('#breadcrumbs-dept')
    $breadcrumbDepartment && $breadcrumbDepartment.addEventListener('click', function () {
        window.amplitudeService.trackCtaClicked({
            actionType: 'Link',
            text: 'Breadcrumb: ' + $breadcrumbDepartment.innerText,
            intent: 'redirect',
            destinationURL: $breadcrumbDepartment.href,
        })
    })

    var $breadcrumbCourse = document.querySelector('#breadcrumbs-course')
    $breadcrumbCourse && $breadcrumbCourse.addEventListener('click', function () {
        window.amplitudeService.trackCtaClicked({
            actionType: 'Link',
            text: 'Breadcrumb: ' + $breadcrumbCourse.innerText,
            intent: 'redirect',
            destinationURL: $breadcrumbCourse.href,
        })
    })

    var $documentInfoSchool = document.querySelector('a[data-cha-target-name=tagged_school_link]')
    $documentInfoSchool && $documentInfoSchool.addEventListener('click', function() {
        window.amplitudeService.trackCtaClicked({
            actionType: 'Link',
            text: 'Document Info: ' + $documentInfoSchool.innerText,
            intent: 'redirect',
            destinationURL: $documentInfoSchool.href,
        })
    })

    var $documentInfoCourse = document.querySelector('#sidebar-course-crosslink')
    $documentInfoCourse && $documentInfoCourse.addEventListener('click', function(){
        window.amplitudeService.trackCtaClicked({
            actionType: 'Link',
            text: 'Document Info: ' + $documentInfoCourse.innerText,
            intent: 'redirect',
            destinationURL: $documentInfoCourse.href,
        })
    })

    var $viewMoreRelated = document.querySelector('#view-more-related')
    $viewMoreRelated && $viewMoreRelated.addEventListener('click', function () {
        window.amplitudeService.trackCtaClicked({
            actionType: 'Link',
            text: 'View More Recommendations',
            intent: 'redirect',
            destinationURL: $viewMoreRelated.href,
            xpath: 'Locked Document Sidebar',
        })
    })

    document.querySelectorAll('.ch_documents_list-item .document-thumbnail-item').forEach(function (recommendedSidebarDocument) {
        recommendedSidebarDocument.addEventListener('click', function() {
            window.amplitudeService.trackRelatedContentClicked({
                destinationUrl: recommendedSidebarDocument.href,
                userState: 'logged out',
                containerLocation: recommendedSidebarDocument.dataset.chaLocation,
                itemIndex: recommendedSidebarDocument.dataset.chaRecommendationListPosition,
                recAlgorithm: recommendedSidebarDocument.dataset.chaRecommendationListAlgorithm,
                containerTitle: recommendedSidebarDocument.dataset.chaRecommendationListName,
            })
        })
    })

    // Track related content clicks in SSI
    var sidebarBlock = document.querySelector('#block_sidebar');
    if (sidebarBlock) {
        document.querySelector('#block_sidebar').addEventListener('click', function (evt) {
            var recommendedSidebarDocument = evt.target.closest('.document-list-card, .document-grid-card');
            if (!recommendedSidebarDocument) {
                return;
            }
            window.amplitudeService.trackRelatedContentClicked({
                destinationUrl: recommendedSidebarDocument.querySelector('.card-link').href,
                userState: 'logged out',
                containerLocation: recommendedSidebarDocument.dataset.chaLocation,
                itemIndex: recommendedSidebarDocument.dataset.chaRecommendationListPosition,
                recAlgorithm: recommendedSidebarDocument.dataset.chaRecommendationListAlgorithm,
                containerTitle: recommendedSidebarDocument.dataset.chaRecommendationListName,
                pageType: recommendedSidebarDocument.dataset.chaPageType,
                pageId: recommendedSidebarDocument.dataset.chaPageId,
                destinationId: recommendedSidebarDocument.dataset.chaDestinationId,
            })
        })
    }

    var otherRelatedMaterialsBlock = document.querySelector('#block_otherRelatedMaterials');
    if (otherRelatedMaterialsBlock) {
       otherRelatedMaterialsBlock.addEventListener('click', function (evt) {
            var recommendedSidebarDocument = evt.target.closest('.document-list-card, .document-grid-card');
            if (!recommendedSidebarDocument) {
                return;
            }
            window.amplitudeService.trackRelatedContentClicked({
                destinationUrl: recommendedSidebarDocument.querySelector('.card-link').href,
                userState: 'logged out',
                containerLocation: recommendedSidebarDocument.dataset.chaLocation,
                itemIndex: recommendedSidebarDocument.dataset.chaRecommendationListPosition,
                recAlgorithm: recommendedSidebarDocument.dataset.chaRecommendationListAlgorithm,
                containerTitle: recommendedSidebarDocument.dataset.chaRecommendationListName,
                pageType: recommendedSidebarDocument.dataset.chaPageType,
                pageId: recommendedSidebarDocument.dataset.chaPageId,
                destinationId: recommendedSidebarDocument.dataset.chaDestinationId,
            })
        })
    }


    // New tracker for View more button in ssi
    document.querySelectorAll('[aria-label="View more related documents"]').forEach(function (recommendedSidebarViewMore) {
        recommendedSidebarViewMore.addEventListener('click', function() {
            window.amplitudeService.trackCtaClicked({
                actionType: 'Link',
                text: 'View More Recommendations',
                intent: 'redirect',
                destinationURL: recommendedSidebarViewMore.dataset.chaHref,
                xpath: 'Locked Document Sidebar',
            })
        })
    })

    document.querySelectorAll('[data-cha-action-type=social_share]').forEach(function(socialBtn) {
        socialBtn.addEventListener('click', function() {
            window.amplitudeService.trackCtaClicked({
                actionType: 'Link',
                text: socialBtn.dataset.chaTargetName.replace('_btn', ''),
                intent: 'Social share',
            })
        })
    })
})
</script>
