<script type="text/javascript">

    function OptanonWrapper() {
        function setConsentTracking(url) {
            if (window.jQuery) {
                jQuery.ajax({
                    url: url,
                    success: function(data) {
                        if (data.success) {
                            console.info(`Consent tracking updated successfully [${data.queryString}].`);
                        } else {
                            console.warn("Error when updating consent tracking.");
                        }
                    }
                });
            }
        }

        let prevActiveGroups = sessionStorage.getItem('activeGroups'),
            reload = (prevActiveGroups) ? (prevActiveGroups !== OptanonActiveGroups) : false;

        sessionStorage.setItem('activeGroups', OptanonActiveGroups);

        var regCheckSocial = /,C0005,/;
        if (OptanonActiveGroups.match(regCheckSocial)) {
            if (document.cookie.split(';').filter((item) => item.includes('dw_dnt=1')).length) {
                setConsentTracking("https://eu.patagonia.com/on/demandware.store/Sites-patagonia-eu-Site/en_GB/ConsentTracking-SetSession?consent=true"); 
            }
        } else {
            if (document.cookie.split(';').filter((item) => item.includes('dw_dnt=0')).length) {
                setConsentTracking("https://eu.patagonia.com/on/demandware.store/Sites-patagonia-eu-Site/en_GB/ConsentTracking-SetSession?consent=false");
            }
        }

        if (reload) {
            sessionStorage.setItem('launchCountryModal', 'true');
            window.location.reload();
        }
    }
</script>

