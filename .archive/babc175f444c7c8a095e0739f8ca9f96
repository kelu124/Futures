			}
		});
		document.removeEventListener('DOMContentLoaded', mccEventLoader);
	} catch (e) { console.error(e); }
};
if (document.readyState === 'complete') {
	mccEventLoader();
} else {
	document.addEventListener('DOMContentLoaded', mccEventLoader);
}
