			<script type="text/javascript">
			(function() {
				var extend = function(out) {
					out = out || {};

					for (var i = 1; i < arguments.length; i++) {
						if (!arguments[i])
						continue;

						for (var key in arguments[i]) {
						if (arguments[i].hasOwnProperty(key))
							out[key] = arguments[i][key];
						}
					}

					return out;
				};
				extend( window.infiniteScroll.settings.scripts, ["jquery","darkmode","jquery-core","jquery-migrate","the-neverending-homepage","disqus_count","disqus_embed","frontend","pushup"] );
				extend( window.infiniteScroll.settings.styles, ["wp-block-library","mediaelement","wp-mediaelement","elasticpress-related-posts-block","styles","global-styles","critical","the-neverending-homepage"] );
			})();
