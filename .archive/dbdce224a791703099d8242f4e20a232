	color: #EF7D1B;
	font-size: 20px;
	font-weight: 500;
}

.uabb-onsale, .onsale {
		background-color: #EF7D1B !important;
}

.onsale {
	  max-height: 3.236em;
    max-width: 3.236em;
    line-height: 3 !important;
}

/* WooCommerce Buttons */
.quantity input {
	padding: 13.5px 12px !important;
	background-color: #fff;
	border: 1px solid #000;
	margin-right: 0px;
	width: 100px !important;
}

