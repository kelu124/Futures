                            function resize() {
                                window.frameElement.height = document.body.offsetHeight;
                            }
                            window.addEventListener(&#x27;resize&#x27;, resize);
                            resize();
                        &lt;/script&gt;&lt;script&gt;
                            var fonts = [].slice.apply(window.parent.document.styleSheets)
