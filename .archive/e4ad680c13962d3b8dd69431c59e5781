<script>
    function resizeEmbeds() {
        const iframes = document.querySelectorAll('.gel-embed');

        for (var i = 0; i < iframes.length; ++i) {
            iframes[i].contentWindow.postMessage({}, '*');
        }
    }

    function updateEmbed(e) {
        let message = e.data;

        let iframe = document.querySelector('.gel-embed[data-connect="' + message.id +'"]');

        if (iframe !== null) {
            iframe.style.height = message.height + 'px';
        }
    };

    window.addEventListener('resize', resizeEmbeds);
    window.addEventListener('message', updateEmbed, false);
</script>
