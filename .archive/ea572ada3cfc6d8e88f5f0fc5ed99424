<script>
  (function () {
    if (!document) return;
    const relList = document.createElement("link").relList;
    if (relList && !relList.supports("preload")) {
      const link = document.querySelector("link[rel='preload'][as='style'][crossorigin]");
      link.rel = "stylesheet";
      ["as", "crossorigin", "onload"].forEach(function (name) { link.removeAttribute(name); });
    }
  })();
</script>

    <script type="text/javascript">
  /* cutoff value is set to March 9th, 2022 6:29:15 PM PST, when Consent Manager was disabled due to issues with default privacy regime.
  This script is to reset the consent of users that viewed the site during the time that Consent Manager was enabled.
  */
  function resetConsent() {
    const auth=event, cutoff=1646879355000;
    if (Date.parse(window.airgap.getConsent().timestamp) < cutoff)
      window.airgap.reset(auth)
  };

  let userSpecifiedImageArea;
  const containsUserSpecifiedImage = (event) => {
    if (!userSpecifiedImageArea) {
      if (!(userSpecifiedImageArea = document.querySelector(".routerContentStory-storyBody"))) {
          return false;
      };
    }
    return event.target instanceof Node && userSpecifiedImageArea.contains(event.target);
  };

  self.airgap = {
    suppressTelemetry: {
      cookies: [
        /^dd_cookie_test_/,
        /^hold_reserves_draft_message_/, 
        /^tag_feedback_/,
        /^category_feedback_/,
        /^_e53a124bebdc/,
        /^_gac_/,
        /^amp_/,
        /^tldtest__/,
        /^Hm_ck_/,
        /^hp2_hld/
      ],
      requestConditions: [
        containsUserSpecifiedImage
      ],
    },
    overrides: [{
      override: (event) => {
        if (containsUserSpecifiedImage(event)) {
          event.allow();
        }
      }
    }],
  };
</script>

