    window.didomiOnReady = window.didomiOnReady || [];
    window.didomiOnReady.push(function (Didomi) {

        if (document.cookie.indexOf('euconsent') < 0) {
            Didomi.reset();
        }

        let mixpanelAuthorization = (Didomi.getUserConsentStatusForVendor("c:mixpanel-UgqczrWa") === undefined) ? true : Didomi.getUserConsentStatusForVendor("c:mixpanel-UgqczrWa");
        let gtmAuthorization = (Didomi.getUserConsentStatusForVendor("google") === undefined) ? true : Didomi.getUserConsentStatusForVendor("google");
        let gaAuthorization = (Didomi.getUserConsentStatusForVendor("google") === undefined) ? true : Didomi.getUserConsentStatusForVendor("google");

        if (Didomi.getUserConsentStatusForPurpose('analytics') === false) {
            mixpanelAuthorization = false;
            gtmAuthorization = false;
            gaAuthorization = false;
        }

        window.didomiEventListeners = window.didomiEventListeners || [];
        window.didomiEventListeners.push({
            event: 'preferences.clickdisagreetoall',
            listener: function () {
                // The user has clicked on the "Disagree to all" button
                mixpanelAuthorization = false;
                gtmAuthorization = false;
                gaAuthorization = false;
            }
        });

        window.didomiEventListeners.push({
            event: 'preferences.clickvendordisagree',
            listener: function (data) {
                // The user has disabled a specific vendor
                // `data.vendorId` contains the ID of the vendor

                switch (data) {
                    case "c:mixpanel-UgqczrWa": {
                        mixpanelAuthorization = false;
                        break;
                    }

                    case "google": {
                        gtmAuthorization = gaAuthorization = false;
                    }
                }
            }
        });

        window.didomiEventListeners.push({
            event: 'preferences.clickagreetoall',
            listener: function () {
                // The user has clicked on the "Agree to all" button
                mixpanelAuthorization = (Didomi.getUserConsentStatusForVendor("c:mixpanel-UgqczrWa") === undefined) ? true : Didomi.getUserConsentStatusForVendor("c:mixpanel-UgqczrWa");
                gtmAuthorization = gaAuthorization = (Didomi.getUserConsentStatusForVendor("google") === undefined) ? true : Didomi.getUserConsentStatusForVendor("google");
            }
        });

    });
</script>
