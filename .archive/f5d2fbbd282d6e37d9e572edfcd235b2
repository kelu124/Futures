<script>
    // check for adblockers and display notice if found
    (function() {
        if (hasClass(document.body, 'page')) {
            // for static Pages like the About page, don't do anything
            // (we only show the notice for article pages and landing pages)
            return
        }
        var ad = document.createElement('div')
        ad.innerHTML = '&nbsp;'
        ad.className = 'AdSense'
        document.body.appendChild(ad)
        var adBlockEnabled = !ad.clientHeight
        if (adBlockEnabled) {
            // redisplay content that adblocker hid
            document.querySelector('.page.wrapper').className = ''

            // create the adblock notice element
            var tmpDiv = document.createElement('div')
            tmpDiv.innerHTML = document.getElementById(
                'artnetMarketingTemplate'
            ).innerHTML
            var notice = tmpDiv.firstElementChild

            // show the notice
            var firstArticle = document.querySelector('.article-body')
            if (firstArticle) {  // article page
                var noticeAndFadeContainer = document.createElement('div')
                noticeAndFadeContainer.className = 'artnet-marketing-notice-container'
                var fadeBox = document.createElement('div')
                fadeBox.className = 'fade-box'
                noticeAndFadeContainer.appendChild(fadeBox)
                noticeAndFadeContainer.appendChild(notice)
                // put the notice at the bottom of the first article
                firstArticle.appendChild(noticeAndFadeContainer)
            }
            else {
                var mainContent = document.getElementById('content')
                var container = document.createElement('div')
                container.className = 'container'
                container.appendChild(notice)
                mainContent.insertBefore(container, mainContent.firstChild)
                var loadMore = document.querySelector('.load-more')
                if (loadMore) {
                    // remove load more button since it might not work correctly with ad blockers
                    loadMore.parentNode.removeChild(loadMore)
                }
            }
        }

        function hasClass(el, className) {
            return ((' ' + el.className + ' ').indexOf(' ' + className + ' ') != -1)
        }
    })()
</script><!-- Instagram Feed JS -->
