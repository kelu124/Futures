<!-- Global_JavaScript Head-->
<script>
//EME / Events / Globals
var listen = listen || {};
listen.ScreenResized;
listen.EmeApplied = 0;
listen.EmeCompleted = 0;
//EME / Events / Delegated
$(function() {
  $('#eme-fixed-banner').on('click','.eme-top-container', emeShowOffer);
  $('#eme-fixed-banner').on('click','a.eme-trigger, .eme-wrapper', emeClick);
});
// HELPER / Get DLV
function getDataLayerVal(keyVal) {
  for (var i = 0; i < window.dataLayer.length; i++) {
    if (dataLayer[i][keyVal] !== undefined) {
      return dataLayer[i][keyVal];
    }
  }
}
// HELPER / Basic throttle
function throttle (callback, limit) {
  var wait = false;
  return function () {
    if (!wait) {
      callback.call();
      wait = true;
      setTimeout(function () {
        wait = false;
      }, limit);
    }
  };
}
//HELPER / Debounce
function debounce(callback, interval) {
  let debounceTimeoutId;

  return function(...args) {
    clearTimeout(debounceTimeoutId);
    debounceTimeoutId = setTimeout(() => callback.apply(this, args), interval);
  };
}
//HELPER / Set cookie
function setCookie(cname, cvalue, exdays) {
  var d = new Date();
  d.setTime(d.getTime() + (exdays * 24 * 60 * 60 * 1000));
  var expires = "expires="+d.toUTCString();
  document.cookie = cname + "=" + cvalue + ";" + expires + ";path=/";
}
//HELPER / Get cookie
function getCookie(cname) {
  var name = cname + "=";
  var ca = document.cookie.split(';');
  for(var i = 0; i < ca.length; i++) {
    var c = ca[i];
    while (c.charAt(0) == ' ') {
      c = c.substring(1);
    }
    if (c.indexOf(name) == 0) {
      return c.substring(name.length, c.length);
    }
  }
  return "";
}
//HELPER / Load script to <head> (and ignore OneTrust)
function loadScript(src) {
  var script = document.createElement("script");
  script.type = "text/javascript";
  script.setAttribute("async", "true");
  script.setAttribute("src", src);
  script.setAttribute("data-ot-ignore","");
  document.getElementsByTagName('head')[0].appendChild(script);
}
//ONETRUST / Check OneTrust cookie consent
function otCookieCheck(category) {
  var otCookie = decodeURIComponent(getCookie('OptanonConsent'));
    return otCookie.indexOf(category+":1") > -1
}
//ONETRUST / Tag parent of embedded content iframe
function otEmbeddedContent() {
	var networkList = {
		"youtube": "YouTube",
		"flo.uri.sh": "Flourish",
		"tiktok": "TikTok",
		"twitter": "Twitter",
      "flickr": "Flickr",
		"vimeo": "Vimeo"
	};
	var frames = document.querySelectorAll('iframe');
	for (var key in networkList) {
		if (networkList.hasOwnProperty(key)) {
			frames.forEach(function(element) {
				var src = element.getAttribute('src') || element.getAttribute('data-src') || "";
				if (src.indexOf(key) > -1) {
					element.parentNode.classList.add('ot-embed-content');
					element.parentNode.setAttribute('data-provider', networkList[key]);
				}
			});
		}
	}
}
//ONETRUST / Show cookie footer
function otShowCookieFooter() {
	var geo = OneTrust.getGeolocationData();
	var otCookieFooter = document.getElementById('ot-cookie-footer');
	if (geo == 'ca' || geo == 'br') {
	otCookieFooter.querySelector('li[data-locale="' + geo + '"]').style.display = 'block';
    otCookieFooter.style.display = 'block';   
	}
	else {
	otCookieFooter.querySelector('li[data-locale="global"]').style.display = 'block';
   otCookieFooter.style.display = 'block'; 
	}
}
//IP / Name tag sizing
function ipNameSize() {
  var rscSW = $('#rsc-sleeve').width();
  var rscLW = $('#rsc-list').width();
  var ipNameS = rscSW-40;
  var ipNameL = rscSW-rscLW-190;
    if($(window).innerWidth() >= 768) {
      $('#ip-auth--name').css('max-width', ipNameL);
    } else {
      $('#ip-auth--name').css('max-width', ipNameS);
    }
}
// IP / Show name tag
function showIpName() {
  if (IPState.useripstate) {
    $("#ip-auth0").show();
    $("#rsc-logo-link").addClass("rsc-bar-ip");
    $("#ip-auth--name").append("<p>" + IPState.useripname + "</p>");
    ipNameSize();
  }
}
//IP / Toggle IP name tag
function toggleIpName() {
  $("#ip-auth0").click(function(){
    $("#ip-auth--name").animate({
      width: "toggle"
    });
  });
}
//DOM / Reorder most block tabs
function mostBlock() {
  var mostBlock = $(".mostThis");
  $(mostBlock).each(function(){
    var blockLate = $(this).find($(".block:contains('Latest')"));
    var blockPop = $(this).find($(".block:contains('Popular')"));
    $(blockPop).insertAfter(blockLate);
  });
}
//DOM / Accordion
function accordionToggle() {
  $('.accordion').each(function(i, el) {
    $(el).on('click', function() {
      $(this).toggleClass('active');
    });
  });
}
//DOM / Sign out behaviour
function signoutRetUrl() {
  $('body').find('a.signout-append-return').each(function(){
    if (!$(this).hasClass('returl-appended')) {
      this.href = this.href + 
      (this.href.indexOf("?") >= 0 ? "&" : "?") + 
      "rtn=" + encodeURIComponent(location.href.replace("?adredir=1", "").replace("&adredir=1", ""));
		$(this).addClass('returl-appended');
	 }
  });
}
//EME / Globals
var eme = eme || {};
eme.LastScroll = 0;
eme.StoppedScrolling;
eme.BannerScroll = throttle(emeShrinkOnScroll,100);
//EME / Add scroll listener
function emeAddScrollListener(){
  $(window).off('scroll', eme.BannerScroll);
  if (window.innerWidth < 1024) {
    $(window).on('scroll', eme.BannerScroll);
  } else {
    $(window).off('scroll', eme.BannerScroll);
  }
}
// EME / Scroll behaviour
function emeShrinkOnScroll(){
  var st = $(window).scrollTop();
  var hasScrolled = (st > eme.LastScroll);
  var emeBar = $('.eme-wrapper');
  if (hasScrolled) {
    emeBar.addClass('eme-shrink');
    emeBar.removeClass('eme-show-offer');
  } else {
     emeBar.removeClass('eme-shrink');
    }
  eme.LastScroll = st;
  window.clearTimeout(eme.StoppedScrolling);
  eme.StoppedScrolling = setTimeout(function() {
    emeBar.removeClass('eme-shrink');
  }, 3000);
}
// EME / Show offer container
function emeShowOffer() {
  $('.eme-wrapper').toggleClass('eme-show-offer')
}
//EME / Splash offer once per session
function emeSplashOffer() {
  if (!sessionStorage.emeOfferSplashed) {
    $('.eme-wrapper').addClass('eme-show-offer');
	 $('.eme-wrapper').attr('data-eme-action','close-offer');
    sessionStorage.setItem("emeOfferSplashed", true);
  }
}
// EME / Update data attribute on expand/close
function emeUpdateAttrOnToggle(){
  if ($('.eme-wrapper').hasClass('eme-show-offer')) {
    $('.eme-wrapper').attr('data-eme-action','close-offer')
  } else {
    $('.eme-wrapper').attr('data-eme-action','expand-offer')
	 }
}
// EME / Click tracking
function emeClick() {
  var emeParent = this.closest('.eme-completed');
  var emeDataSet = this.closest('.eme-data-set');
  var str = function(attr) {
    if (attr) {
      attr = attr.charAt(0).toUpperCase() + attr.substr(1).toLowerCase();
      attr = attr.replace(/-/g, ' ');
      return attr;
    };
  };
  window.dataLayer.push({
    'event': 'eme.click',
    'eme.msg': emeParent.getAttribute('data-message'),
    'eme.tribe': str(emeDataSet.getAttribute('data-eme-tribe')),
    'eme.exp': str(emeDataSet.getAttribute('data-eme-exp')),
    'eme.state': str(emeDataSet.getAttribute('data-eme-state')),
    'eme.action': str(this.getAttribute('data-eme-action')),
	 'eme.title': str(this.getAttribute('title')),
	 'eme.text': str(this.innerText)
  });
  emeUpdateAttrOnToggle();
}
// EME / Event handling
function emeEventHandler() {
  if (!listen.EmeApplied) {
    emeAddScrollListener();
    console.log('eme item applied');
    listen.EmeApplied++;
  }
}
// EME / Process msg content
function emeMsgProcessing() {
  emeRetUrl();
  emeIpRetUrl();
  signoutRetUrl();
  listen.EmeCompleted++;
}
// EME / Append return url
function emeRetUrl() {
  $('.eme-completed').find('a.ad-append-return').each(function(){
    if (!$(this).hasClass('returl-appended')) {
      this.href = this.href + (this.href.indexOf("?") >= 0 ? "&" : "?") + "returl=" + encodeURIComponent(location.href.replace("?adredir=1", "").replace("&adredir=1", ""));
	  $(this).addClass('returl-appended');
    }
  });
}
// EME / Append IP name and return url
function emeIpRetUrl() {
    $('.eme-completed').find('a.ip-append-return').each(function(){
    if (!$(this).hasClass('returl-appended')) {
    $(this).attr("href", "https://account.chemistryworld.com/register?campaigncode=CWemeipreg2109&ipcompany=" + encodeURIComponent(IPState.useripname) + "&returl=" + encodeURIComponent(location.origin + location.pathname));
	 $(this).addClass('returl-appended');
  }
});
}
// EME / Event listeners
$(window).on('eme:itemapplied', emeEventHandler);
$(window).on('eme:itemcompleted', emeMsgProcessing);
$(window).on('eme:itemcompleted', emeSplashOffer);

// SCHEMA / FAQ
function faqSchema() {
  if (
  $('body#story').length && 
  getDataLayerVal('CategoryCodes').indexOf('|568|')>-1
  ) {
    var data = {
      "@context":"https://schema.org",
      "@type":"FAQPage",
      "mainEntity":[]
    };
    var storyheadings = $('.storytext h2, .storytext h3');
    storyheadings.each(function(){
      data.mainEntity.push({
        "@type":"Question",
        "name":$(this).text(),
        "acceptedAnswer": {
          "@type": "Answer",
          "text":$(this).nextAll('p:first').text()
        }
      });
    });
    var script = document.createElement('script');
    script.type = "application/ld+json";
    script.innerHTML = JSON.stringify(data);
    document.getElementsByTagName('body')[0].appendChild(script);
  }
}
//SCHEMA / Video
function videoSchema() {
  if (
  $('body#story').length && 
  getDataLayerVal('StoryLayout') === 'Video'
  ) {
    var metaimage = document.querySelector('meta[property~="og:image"]');
    var thumbnail = metaimage && metaimage.getAttribute("content");
    var publishdate = getDataLayerVal('PublishDate')
    var uploadDate = publishdate.split("-").reverse().join("-");
    var videoiframe = $('.videoblock iframe')[0];
    var embedUrl = videoiframe.getAttribute('src');
    var data = {
      "@context": "https://schema.org",
      "@type": "VideoObject",
      "name": getDataLayerVal('Headline'),
      "description": getDataLayerVal('Intro'),
      "thumbnailUrl": [
        thumbnail
       ],
      "uploadDate": uploadDate,
      "embedUrl": embedUrl
    };
    var script = document.createElement('script');
    script.type = "application/ld+json";
    script.innerHTML = JSON.stringify(data);
    document.getElementsByTagName('body')[0].appendChild(script);
  }
}
// SCHEMA / Event
function eventSchema() {
  if (
  $('body#story').length && 
  getDataLayerVal('StoryType') === 'Webinar' && getDataLayerVal('CategoryCodes').indexOf('|580|')< 0
  ) {
    var metaimage = document.querySelector('meta[property~="og:image"]');
	var imageUrl = metaimage && metaimage.getAttribute("content");
    var eventDate = $('.factfile time')[0];
	var startDate = eventDate.getAttribute('datetime');
    var data = {
      "@context": "https://schema.org",
      "@type": "Event",
      "name": getDataLayerVal('Headline'),
	  "startDate": startDate,
      "eventStatus": "https://schema.org/EventScheduled",
      "eventAttendanceMode": "https://schema.org/OnlineEventAttendanceMode",
	  "isAccessibleForFree": true,
	  "location": {
        "@type": "VirtualLocation",
        "url": $("link[rel='canonical']").attr("href")
        },
	  "image": [
        imageUrl
       ],	
      "description": getDataLayerVal('Intro'),
      "performer": {
        "@type": "Organization",
        "name": getDataLayerVal('FirstByline'),
      },	  
	  "sponsor": {
        "@type": "Organization",
        "name": getDataLayerVal('FirstByline'),
      },
      "organizer": {
        "@type": "Organization",
        "name": "Chemistry World",
        "url": "https://www.chemistryworld.com"
      }
    };
    var script = document.createElement('script');
    script.type = "application/ld+json";
    script.innerHTML = JSON.stringify(data);
    document.getElementsByTagName('body')[0].appendChild(script);
  }
}
// AJAX / Staff comment badge
function staffComment() {
  var staff = [
    'https://www.chemistryworld.com/katrina/63.publicprofile',
    'https://www.chemistryworld.com/jennifer/2787.publicprofile',
    'https://www.chemistryworld.com/jamie/11260.publicprofile',
    'https://www.chemistryworld.com/adam/70.publicprofile',
    'https://www.chemistryworld.com/phillip/62.publicprofile',
	'https://www.chemistryworld.com/frances/95495.publicprofile',
	'https://www.chemistryworld.com/neil-withers/57.publicprofile',
	'https://www.chemistryworld.com/pat/68.publicprofile'
  ];
  var profile = $('#comments').find('p.byline>a:first-of-type');
  profile.each(function(){
    if (jQuery.inArray($(this).attr('href'), staff) != -1)  {
      $(this).parent().append("<span class='staffComment'>Staff</span>" )
    }
  })
}
</script>

<!-- Additional DLV -->
<script>
var dlv = dlv || {};
dlv = (function() {
  var userSubscriber = getDataLayerVal('Products')!==undefined;
  var userLoggedIn = getDataLayerVal('LoggedIn') == 'True';
  var userMember = getDataLayerVal('IMIS_Active') == "|true|";
  var userIP = IPState.useripstate === true;
  var userSubState = ((!userLoggedIn && userIP) ? 'IP signed out' : (!userLoggedIn) ? 'Casual' : (userMember) ? 'Member' : (userSubscriber) ? 'Subscriber' : (userLoggedIn && userIP) ? 'IP signed in' : 'Registered');
  var userState = getDataLayerVal('LoggedIn') == 'True' ? 'Signed in' : 'Signed out';
  var firstByline = getDataLayerVal('Bylines') != undefined ? getDataLayerVal('Bylines').replace(/^\|/, '') : undefined;
  firstByline = firstByline !=undefined ? firstByline.replace(/(\|.*$)/, '') : undefined;
  return {
    userState: userState,
    userSubState: userSubState,
	 firstByline: firstByline
    };
})();
window.dataLayer.push({'UserState':dlv.userState});
window.dataLayer.push({'UserSubState':dlv.userSubState});
window.dataLayer.push({'FirstByline':dlv.firstByline});
</script>

