<script>
function showMssgBlock2(){
  var mb__exists = document.querySelector('.mb__control') !== null;
  var accessGranted = getDataLayerVal('AccessStatus') === 'Granted';
  var validPage = getDataLayerVal('Template') === 'Story' || getDataLayerVal('Template') === 'SPIN';
  var navPath = getDataLayerVal('FullNavPath').split("|");
  var validNav = (navPath.indexOf('59') == -1) && (navPath.indexOf('60') == -1) && (navPath.indexOf('63') == -1);
  var showMssgBlock = ((validPage) && (validNav) && (accessGranted) && (!IPState.useripstate));
  var hasMBCookie = getCookie('MB__closed') == 'True';
  if ((showMssgBlock) && (mb__exists) && (!hasMBCookie)) {
    var mb__trigger = document.getElementsByClassName("mb__control")[0].innerHTML;
    $("#MB_Pos").load("/Uploads/l/v/r/mb_content_20210427_130127.html #" + mb__trigger, function(){
	   mbevent('Shown');
	 });
  }
}

function closeMssgBlock2(){
  $('#MB_Pos').on('click', '.mb__close', function(){
    setCookie("MB__closed","True",1);
    $(".mb_bottom_fixed").slideUp(300);
	 mbevent('Closed');
  });
}

function clickMssgBlock(){
  $('#MB_Pos').on('click', '.mb_main_cta .button', function(){
	 mbevent('Clicked');
  });
}

function mbevent(action){
  var mbType = $('#MB_Pos').children().attr('id');
  window.dataLayer.push({
    'event': 'mbevent',
    'mbCat': 'Message block',
    'mbAct': action,
    'mbLab': mbType,
  });
}

function retUrls(){
$('.mb_bottom_fixed').find('a.ad-append-return').each(function(){
    this.href = this.href + (this.href.indexOf("?") >= 0 ? "&" : "?") + "returl=" + encodeURIComponent(location.href.replace("?adredir=1", "").replace("&adredir=1", ""))
});}

function mssgHandler() {
  var mutateCount = 0;
  var observer = new MutationObserver(function (mutations, observer) {
    mutations.forEach(function (mutation) {
      [].slice.call(mutation.addedNodes).forEach(function (addedNode) {
        if (addedNode.classList.contains('mb_bottom_fixed') && mutateCount === 0) {
          mutateCount ++;
          closeMssgBlock2();
			 clickMssgBlock();
          retUrls();
          ipNameParam();
          observer.disconnect();
        }
      });
    });
	 observer.disconnect();
  });
