}

function ipNameParam() {
    if ($('a#ipregister').length) {
      var ipNameURI = encodeURIComponent(IPState.useripname);
      var ipRegURL = "https://account.chemistryworld.com/register?campaigncode=CWRIP1704&ipcompany=" + ipNameURI + "&returl=" + encodeURIComponent(location.href);
    $("a#ipregister").attr("href", ipRegURL);
    }
}
  
