  var accessGranted = getDataLayerVal('AccessStatus') == "Granted";
  var storyPageGranted = ((storyPage) && (accessGranted));
  var storyLayout = getDataLayerVal('StoryLayout');
  var showAdsA_B = ((navPage) && (navPath.indexOf('63') == -1)) || ((!navPage) && (accessGranted) && (navPath.indexOf('63') == -1));
